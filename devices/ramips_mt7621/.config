CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_MULTI_PROFILE=y
CONFIG_TARGET_ROOTFS_INITRAMFS=y
CONFIG_TARGET_DEVICE_ramips_mt7621_DEVICE_xiaomi_redmi-router-ac2100=y

CONFIG_PACKAGE_luci-ssl=y # uhttpd服务
CONFIG_PACKAGE_kmod-fs-ntfs3-oot=n
# CONFIG_PACKAGE_kmod-fs-ntfs3-oot is not set

# 自定义设置:
CONFIG_PACKAGE_luci-app-ramfree=y

# Bypass插件包:
CONFIG_PACKAGE_luci-app-bypass=y
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_Shadowsocks_Server=n
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_Simple_obfs=n
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_V2ray_plugin=n
# CONFIG_PACKAGE_luci-app-bypass_INCLUDE_Trojan-Go is not set
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_NaiveProxy=n
# CONFIG_PACKAGE_luci-app-bypass_INCLUDE_Kcptun is not set


# 其他需要安装的软件包:
CONFIG_PACKAGE_bind-host=y
CONFIG_PACKAGE_knot-host=y

# 第三方插件选择:
CONFIG_PACKAGE_luci-app-jd-dailybonus=y
CONFIG_PACKAGE_luci-app-arpbind=y
CONFIG_PACKAGE_luci-app-openclash=y 
CONFIG_PACKAGE_luci-app-serverchan=y
CONFIG_PACKAGE_luci-app-eqos=y
CONFIG_PACKAGE_luci-app-adguardhome=y
CONFIG_PACKAGE_luci-app-socat=y
CONFIG_PACKAGE_luci-app-frpc=y
CONFIG_PACKAGE_luci-app-mwan3=y
CONFIG_PACKAGE_luci-app-mwan3helper=y
CONFIG_PACKAGE_luci-app-syncdial=y
CONFIG_PACKAGE_luci-app-ttyd=y
CONFIG_PACKAGE_luci-app-webadmin=y
CONFIG_PACKAGE_luci-app-zerotier=y
CONFIG_PACKAGE_luci-app-ddns=y
CONFIG_PACKAGE_ddns-scripts_aliyun=y
CONFIG_PACKAGE_ddns-scripts_dnspod=y
CONFIG_PACKAGE_ddns-scripts-aliyun=y
CONFIG_PACKAGE_ddns-scripts-dnspod=y
CONFIG_PACKAGE_luci-app-vlmcsd=y
# CONFIG_UnblockNeteaseMusic_NodeJS is not set
CONFIG_PACKAGE_luci-app-unblockmusic=y
CONFIG_UnblockNeteaseMusic_Go=y
CONFIG_PACKAGE_luci-app-wol=y
CONFIG_PACKAGE_luci-app-filetransfer=y 
CONFIG_PACKAGE_luci-app-accesscontrol=y

